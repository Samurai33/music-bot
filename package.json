{
  "name": "music-bot",
  "version": "1.0.0",
  "type": "module",
  "description": "A simple Discord music bot built with discord.js and discord-player.",
  "scripts": {
    "dev": "tsx src/index.ts",
    "register": "tsx src/register-commands.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node --enable-source-maps dist/index.js"
  },
  "dependencies": {
    "@discord-player/extractor": "^7.1.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.4.0",
    "@discordjs/voice": "^0.19.0",
    "discord-player": "^7.1.0",
    "discord-player-youtubei": "^1.5.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "opusscript": "^0.0.8",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
